{
  "name": "promethee",
  "version": "0.1.0",
  "keywords": [
    "PROMETHEE",
    "MCDA",
    "Preference Ranking Organization METHod for Enrichment Evaluation"
  ],
  "description": "A simplified package for calculating the multi-criteria decision analysis methods PROMETHEE I and II",
  "main": "index.js",
  "repository": "git@github.com:GabrielAlfs/promethee.git",
  "author": "Gabriel Alves <l.luis.gabriel@outlook.com>",
  "license": "MIT",
  "scripts": {
    "dev": "tsnd --respawn --transpile-only -r tsconfig-paths/register ./src/application/index.ts",
    "build": "tsc --project tsconfig.json && tscpaths -p tsconfig.json -s ./src -o ./dist"
  },
  "devDependencies": {
    "@types/node": "^16.10.2",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.4.1",
    "ts-node": "^10.2.1",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.11.0",
    "tscpaths": "^0.0.9",
    "typescript": "^4.4.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
